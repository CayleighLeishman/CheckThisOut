<% if (session.error) { %>
    <div class="alert alert-danger session-flash-alert">
        <%= session.error %>
    </div>
    <% delete session.error; %>
  <% } %>
  
  <% if (session.success) { %>
    <div class="alert alert-success session-flash-alert">
        <%= session.success %>
    </div>
    <% delete session.success; %>
  <% } %>
<!DOCTYPE html>
<html lang="en">
<body>
    <main>
        <section>
            <h2>Personal Information</h2>
            <p><strong>Name:</strong> <%= user.name %></p>
            <p><strong>Email:</strong> <%= user.email %></p>
            <p><strong>Joined:</strong> <%= user. %></p>
        </section>
    </main>
</body>
</html>
